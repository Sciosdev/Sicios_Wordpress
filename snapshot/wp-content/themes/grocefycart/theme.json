      "name": "blank-with-headerfooter",
      "postTypes": ["page"]
    },
    {
      "title": "Blank Page Template",
      "name": "blank",
      "postTypes": ["page"]
    },
    {
      "name": "archive-product",
      "title": "Product Catalog",
      "postTypes": ["product"]
    },
    {
      "name": "cart",
      "title": "WooCommerce Cart Page",
      "postTypes": ["product"]
    },
    {
      "name": "checkout",
